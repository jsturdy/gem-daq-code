    <node id="board_id"  address="0x00000000"  permission="r"
	  description="GLIB board ID code">
      <node id="board_id_char1"  mask="0xff000000"/>
      <node id="board_id_char2"  mask="0x00ff0000"/>
      <node id="board_id_char3"  mask="0x0000ff00"/>
