<?xml version="1.0" encoding="ISO-8859-1"?>
<node id="top">
  <node id="Send"  address="0x0"
	description="send fast command">
    <node id="L1A"          address="0x0"  mask="0xffffffff"  permission="w"
	  description="send LV1A command to system" />
    <node id="CalPulse"     address="0x1"  mask="0xffffffff"  permission="w"
	  description="send CalPulse command to system" />
    <node id="Resync"       address="0x2"  mask="0xffffffff"  permission="w"
	  description="send Resync command to system" />
    <node id="BC0"          address="0x3"  mask="0xffffffff"  permission="w"
	  description="send BC0 command to system" />
    <node id="L1ACalPulse"  address="0x4"  mask="0xffffffff"  permission="w"
	  description="send L1A and CalPulse separated by <value> VFAT2 clock cycles" />
  </node> <!-- end send commands -->
</node> <!-- end fast command definitions -->
