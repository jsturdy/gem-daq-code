    <node id="Mod_abs"     mask="0x00000010"/>
    <node id="RxLOS" 	   mask="0x00000020"/>
    <node id="TxFault"     mask="0x00000040"/>
  </node>

  <node id="SFP3"
	description="status bits for SFP cage 3">
